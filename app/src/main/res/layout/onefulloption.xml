        style="@style/onefulloptstl"

        android:text="baseFeatures" />

    <TextView
        android:id="@+id/viewIndTextView2"
        style="@style/onefulloptstl"


        android:text="viewInd" />

    <TextView
        android:id="@+id/removeIndTextView2"
        style="@style/onefulloptstl"

        android:text="removeInd" />

    <TextView
        android:id="@+id/effDateTextView2"
        style="@style/onefulloptstl"

        android:text="effDate" />

    <TextView
        android:id="@+id/expDateTextView2"
        style="@style/onefulloptstl"

        android:text="expDate" />

    <TextView
        android:id="@+id/archiveIndTextView2"
        style="@style/onefulloptstl"

        android:text="archiveInd" />

</LinearLayout>
