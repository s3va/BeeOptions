<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="4dp">

    <TextView
        android:id="@+id/nameTextView2"
        style="@style/onefulloptstl"
        android:text="name" />

    <TextView
        android:id="@+id/entityNameTextView2"
        style="@style/onefulloptstl"
        android:text="entityName" />

    <TextView
        android:id="@+id/entityDescTextView2"
        style="@style/onefulloptstl"
        android:text="entityDesc" />

    <TextView
        android:id="@+id/rcRateTextView2"
        style="@style/onefulloptstl"


        android:text="rcRate" />

    <TextView
        android:id="@+id/rcRatePeriodTextView2"

        style="@style/onefulloptstl"

        android:text="rcRatePeriod" />


    <TextView
        android:id="@+id/rcRatePeriodTextTextView2"
